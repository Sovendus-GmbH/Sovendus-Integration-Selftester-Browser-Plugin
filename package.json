{
  "name": "sovendus-integration-tester",
  "version": "2.0.0",
  "description": "Sovendus integration tester chrome & firefox extensions",
  "private": false,
  "scripts": {
    "build": "yarn sov-build build",
    "build-release": "yarn run build-and-test",
    "debug-on-ff": "cd ../dist/firefox && web-ext run",
    "lint": "cd .. && eslint integration-tester/src/**/*.ts --no-ignore --fix && eslint integration-tester/src/**/*.tsx --no-ignore --fix",
    "zip-builds": "zip-build ../dist/chrome release_zips -t 'chrome-test-sovendus-integration_%VERSION%_%TIMESTAMP%.%EXT%' && zip-build ../dist/firefox release_zips -t 'firefox-test-sovendus-integration_%VERSION%_%TIMESTAMP%.%EXT%' && zip-build ../dist/chrome test_zips -t 'chrome-test-sovendus-integration_TESTING.crx' && zip-build ../dist/firefox test_zips -t 'firefox-test-sovendus-integration_TESTING.xpi'"
  },
  "author": "Marcus Brandstaetter - Sovendus GmbH",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "lucide-react": "^0.468.0",
    "pdfmake": "^0.2.18",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "sovendus-builder": "^1.0.15",
    "@types/chrome": "^0.0.270",
    "@types/crypto-js": "^4.2.2",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.2",
    "@types/pdfmake": "^0.2.11",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@types/selenium-webdriver": "^4.1.26",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "selenium-webdriver": "^4.24.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "web-ext": "^8.2.0",
    "zip-build": "^1.8.0"
  }
}