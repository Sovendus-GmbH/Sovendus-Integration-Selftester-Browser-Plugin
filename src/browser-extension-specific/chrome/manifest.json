{
  "name": "Sovendus Integration Tester",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "Use this extension to verify if your Sovendus integration is setup properly",
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "permissions": [
    "scripting",
    "webNavigation",
    "storage",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "extensionPopUp.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/browserExtensionLoader.js",
        "/extensionPopUp.js",
        "/extensionPopUp.js.map",
        "/browserExtensionLoader.js.map"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": []
    }
  ],
  "icons": {
    "128": "sovendus.png"
  }
}