{
  "name": "Sovendus Integration Tester",
  "version": "1.7.0",
  "manifest_version": 2,
  "description": "Use this extension to verify if your Sovendus integration is setup properly",
  "background": {
    "scripts": [
      "serviceWorker.js"
    ],
    "persistent": true
  },
  "permissions": [
    "<all_urls>",
    "scripting",
    "clipboardWrite"
  ],
  "icons": {
    "128": "sovendus.png"
  },
  "browser_action": {
    "default_icon": "sovendus.png",
    "default_title": "Sovendus Integration Tester",
    "default_popup": "extensionPopUp.html"
  },
  "applications": {
    "gecko": {
      "id": "{9ea53b13-faa1-4963-bb55-481b5adda7b6}"
    }
  }
}