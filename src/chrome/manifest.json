{
  "name": "Sovendus Integration Tester",
  "version": "1.3.3",
  "manifest_version": 3,
  "description": "Use this extension to verify if your Sovendus integration is setup properly",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "extension-pop-up/export-result-popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/page-banner/self-test-overlay.js",
        "/page-banner/self-test-overlay.js.map",
        "/page-banner/self-tester.js",
        "/page-banner/self-tester.js.map",
        "/src/page-banner/self-test-overlay.ts",
        "/src/page-banner/self-tester.ts"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": []
    }
  ],
  "icons": {
    "128": "sovendus.png"
  }
}